# machine:
#   kubelet:
#     {indent(4, kubelet)}
# 
cluster:
  allowSchedulingOnControlPlanes: true
  # apiServer:
  #   {indent(4, api_server)}
  network:
    cni:
      name: none
%{ if network.podCidr }
    # The pod subnet CIDR.
    podSubnets:
        - ${network.podCidr}
%{ endif }
%{ if network.serviceCidr }
    # The service subnet CIDR.
    serviceSubnets:
        - ${network.serviceCidr}
%{ endif }
  proxy:
    disabled: true
  discovery:
    enabled: true
    registries:
      service:
        disabled: false
  extraManifests: ${extra_manifests}
  inlineManifests: ${inline_manifests}
